algoritmo "mostrar o resultado na tela"
// Função :
// Autor :
// Data : 18/06/2022
// Seção de Declarações 
var
  nome : caractere
  idade, s : inteiro

inicio
  Escreva("Qual é o seu nome:")
  leia(nome)
  Escreva("Qual sua idade:")
  leia(idade)
  Escreva("Qual é o seu sálario:")
  leia(s)
  Escreva("Olá " , nome , " sua idade é de " , idade, " e voce ganha $", s,".")
fimalgoritmo
